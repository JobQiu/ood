          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_lY8PgVZjEeWnF-e2bm8tZQ" name="bounce" specification="_lYCQkVZjEeWnF-e2bm8tZQ">
            <language>java</language>
            <body>if (location.x - radius &lt; 0) {
         velocity.x = -velocity.x;
         location.x += 2 * (radius - location.x);
     }
     
     if (location.y - radius &lt; 0) {
         velocity.y = -velocity.y;
         location.y += 2 * (radius - location.y);
     }
     
     if (location.x + radius > canvas.getWidth()) {
         velocity.x = -velocity.x;
         location.x -= 2 * (location.x - (canvas.getWidth() - radius));
     }
     
     if (location.y + radius > canvas.getHeight()) {
         velocity.y = -velocity.y;
         location.y -= 2 * (location.y - (canvas.getHeight() - radius));
     }
 </body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_lY8PglZjEeWnF-e2bm8tZQ" name="paint" specification="_lYC3oFZjEeWnF-e2bm8tZQ">
