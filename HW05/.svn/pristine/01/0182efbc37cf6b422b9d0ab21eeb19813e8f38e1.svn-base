package model.paint.strategy;


import java.awt.geom.AffineTransform;

import model.painting.ImagePaintStrategy;

/**
 * strategy that paints a running man on the screen
 * @author caojian
 *
 */
public class SonicImagePaintStrategy extends ImagePaintStrategy{

	/**
	 * The path of image file
	 */
	private static final String filename = "images/sonic_animate.gif";
	
	/**
	 * The affine transform object
	 */
	private static AffineTransform at = new AffineTransform();
	
	/**
	 * The fill factor
	 */
	private static double fillFactor = 1.0;
	
	/**
	 * Constructor that loads image
	 */
	public SonicImagePaintStrategy() {
		super(at, filename, fillFactor);
	}

} 
