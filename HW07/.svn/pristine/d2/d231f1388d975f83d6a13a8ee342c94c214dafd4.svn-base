            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2Kn8kXvJEeWoTeiWGqdU6g" name="addTasks" specification="_122E8HvJEeWoTeiWGqdU6g">
              <language>java</language>
              <body>taskTable.put(&quot;chat&quot;, this::twoWayChat);&#xD;
