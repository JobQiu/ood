            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2Krm83vJEeWoTeiWGqdU6g" name="calcServerTime" specification="_13N4YHvJEeWoTeiWGqdU6g">
              <ownedParameter xmi:id="_2Krm9HvJEeWoTeiWGqdU6g" name="p">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <ownedParameter xmi:id="_2Krm9XvJEeWoTeiWGqdU6g" direction="return">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <language>java</language>
              <body>if (null == comp) return &quot;No ICompute object yet!&quot;;&#xD;
		try {&#xD;
			return comp.executeTask(new ServerTime(p)).format(java.time.format.DateTimeFormatter&#xD;
					.ofPattern(&quot;EEE MMM dd, hh:mm:ss a&quot;));&#xD;
		} catch (Exception e) {&#xD;
			e.printStackTrace();&#xD;
			return &quot;Compute ServerTime exception: &quot; + e;&#xD;
		}&#xD;
	</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2KuqQHvJEeWoTeiWGqdU6g" name="executeTask" specification="_13PGgHvJEeWoTeiWGqdU6g">
