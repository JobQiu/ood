package view;

public interface IModelCtrlAdapter {
    
    public void loadBall(String className);
    
    public void clearBalls();
    
    public static final IModelCtrlAdapter NULL_OBJECT = new IModelCtrlAdapter() {
        
        @Override
        public void loadBall(String className) {}
        
        @Override
        public void clearBalls() {}
    };
}
