package model.ball;

import java.awt.Graphics;

import util.Randomizer;

public class WanderBall extends ABall {

    @Override
    protected void move() {
        location.x += velocity.x + Randomizer.Singleton.randomInt(-10, 10);
        location.y += velocity.y + Randomizer.Singleton.randomInt(-10, 10);
    }

    @Override
    protected void paint(Graphics g) {
        // TODO Auto-generated method stub

    }

}
